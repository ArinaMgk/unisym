DD endo
[BITS 32]
MOV BYTE[0xB8001],0x70
PUSH EAX; STAK CHK
POP EAX
MOV ESI,str0
MOV EDI,0xB8000+80*2
MOV ECX,str0e-str0
Print0:
	MOV AL,[ESI]
	MOV [EDI],AL
	INC ESI
	ADD EDI,2
	LOOP Print0
CLI
HLT
str0: db "Hello,world!~~~"
str0e:

endo:
