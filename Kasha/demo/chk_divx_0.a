[CPU 8086]

%include "video.a"
%include "arith.a"
%include "pseudo.a"
File
	CLI
	MOV AX, 0
	MOV SS, AX
	MOV SP, 0X7C00
	STI
	MOV AX,0XB800
	MOV ES,AX
	
	MOV AX,0xFFFF
	MOV BL,1
	DIVX BL
	MOV BX,AX
	MOV AH,0x70
	CALL F_Print16
	MOV AL,' '
	CALL F_ConPrintChar
	XOR DH,DH
	MOV BX,DX
	CALL F_Print16
	
	MOV AL,0x0D
	CALL F_ConPrintChar
	MOV AL,0x0A
	CALL F_ConPrintChar
	MOV DX,0XFEDC
	MOV AX,0X4321
	MOV BX,3
	DIVX BX

	MOV BX,CX
	CALL F_Print16
	MOV BX,AX
	CALL F_Print16
	MOV AL,' '
	CALL F_ConPrintChar
	MOV BX,DX
	CALL F_Print16

	CLI
	HLT
	F_Print16:
		ConPrintWord BX,F_ConPrintChar
		RET
	F_ConPrintChar:
		ConPrintChar AL,0x70,~
		RET
Endf