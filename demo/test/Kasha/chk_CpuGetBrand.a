%include "pseudo.a"
%include "video.a"
%include "cpuins.a"

File
	mov ax,0x0100
	mov ds,ax
	DefineStack16
	mov ax,0xB800
	mov es,ax
	ConCursor 0
	mov esi,Brand
	CpuBrand
	mov esi,Brand
	ConPrint si,0x72; [Green on White]
	CLI
	HLT
	Brand: times 50 db 0
Endf

