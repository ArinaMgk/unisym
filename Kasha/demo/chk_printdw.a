[CPU 386]
; build chk_printdw.a 1
%include "video.a"
%include "arith.a"
%include "pseudo.a"
File
	CLI
	MOV AX, 0
	MOV SS, AX
	MOV SP, 0X7C00
	STI
	MOV AX,0XB800
	MOV ES,AX
	
	MOV EBX,0x98765432
	CALL F_Print16
	MOV EBX,0xFEDCBA10
	CALL F_Print16
	CLI
	HLT
	F_Print16:
		ConPrintDWord EBX,F_ConPrintChar
		RET
	F_ConPrintChar:
		ConPrintChar AL,0x70,~
		RET
Endf