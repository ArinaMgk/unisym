%imacro CpuBrand 0
	MOV EDI,0X80000002
	%%loop:
	MOV EAX,EDI
	INC EDI
	CPUID
	MOV [ESI + 0X00],EAX
	MOV [ESI + 0X04],EBX
	MOV [ESI + 0X08],ECX
	MOV [ESI + 0X0C],EDX
	ADD ESI,0X10
	CMP EDI,0X80000004
	JBE %%loop
%endmacro CpuGetBrand32 1








